<div class="row">
  <div class=" col-sm-12">
    <h1 class="text-center">All users</h1>
    <ul class="users ">
      <% @users.each do |user| %>
        <li class="li_user">
          <%= link_to user.name, user %>
          <%= image_tag user.avatar.url if user.avatar? %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
